<Window x:Class="Login.PassWordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="PassWordWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="user" HorizontalAlignment="Left" Height="23" Margin="272,146,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox Name="password" HorizontalAlignment="Left" Margin="272,191,0,0" VerticalAlignment="Top" Height="23" Width="120"/>
        <Label Content="账号" HorizontalAlignment="Left" Margin="202,146,0,0" VerticalAlignment="Top"/>
        <Label Content="密码&#xA;" HorizontalAlignment="Left" Margin="202,189,0,0" VerticalAlignment="Top"/>
        <Button Content="登陆" HorizontalAlignment="Left" Margin="272,269,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Name="label" Content="" HorizontalAlignment="Left" Margin="233,58,0,0" VerticalAlignment="Top" Width="159"/>

    </Grid>
</Window>
